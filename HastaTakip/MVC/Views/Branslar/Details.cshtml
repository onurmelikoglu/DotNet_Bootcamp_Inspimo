@{
    ViewData["Title"] = "Branş Detayı";
}

<div class="container">

    <h1>@ViewData["Title"]</h1>

    <div>
        <h4>Branş</h4>
        <hr />
        <dl class="row">
            <dt class="col-2">
                @Html.DisplayNameFor(model => model.Adi)
            </dt>
            <dd class="col-10">
                @Html.DisplayFor(model => model.Adi)
            </dd>
        </dl>
        <dl class="row">
            <dt class="col-2">
